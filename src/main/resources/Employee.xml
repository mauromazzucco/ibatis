<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="test.Funcionario" >
	
	<insert id="insert" parameterType="func">
      insert into func(nome, funcao, data)
      VALUES(#{nome}, #{funcao}, #{data}) 

  
   </insert> 
	
</mapper> 